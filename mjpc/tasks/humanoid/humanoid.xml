<mujoco model="Humanoid">

  <statistic extent="2" center="0 0 1"/>

  <default>
    <motor ctrlrange="-1 1" ctrllimited="true"/>
    <site size=".04" group="3"/>
    <default class="body">

      <!-- geoms -->
      <geom type="capsule" condim="1" friction=".7" solimp=".9 .99 .003" solref=".015 1" material="self"/>
      <default class="thigh">
        <geom size=".06"/>
      </default>
      <default class="shin">
        <geom fromto="0 0 0 0 0 -.3"  size=".049"/>
      </default>
      <default class="foot">
        <geom size=".027"/>
        <default class="foot1">
          <geom fromto="-.07 -.01 0 .14 -.03 0"/>
        </default>
        <default class="foot2">
          <geom fromto="-.07 .01 0 .14  .03 0"/>
        </default>
      </default>
      <default class="arm_upper">
        <geom size=".04"/>
      </default>
      <default class="arm_lower">
        <geom size=".031"/>
      </default>
      <default class="hand">
        <geom type="sphere" size=".04"/>
      </default>

      <!-- joints -->
      <joint type="hinge" damping="0.2" stiffness="1" armature=".01" limited="true" solimplimit="0 .99 .01"/>
      <default class="joint_big">
        <joint damping="5" stiffness="10"/>
        <default class="hip_x">
          <joint range="-30 10"/>
        </default>
        <default class="hip_z">
          <joint range="-60 35"/>
        </default>
        <default class="hip_y">
          <joint axis="0 1 0" range="-150 20"/>
        </default>
        <default class="joint_big_stiff">
          <joint stiffness="20"/>
        </default>
      </default>
      <default class="knee">
        <joint pos="0 0 .02" axis="0 -1 0" range="-160 2"/>
      </default>
      <default class="ankle">
        <joint range="-50 50"/>
        <default class="ankle_y">
          <joint pos="0 0 .08" axis="0 1 0" stiffness="6"/>
        </default>
        <default class="ankle_x">
          <joint pos="0 0 .04" stiffness="3"/>
        </default>
      </default>
      <default class="shoulder">
        <joint range="-85 60"/>
      </default>
      <default class="elbow">
        <joint range="-100 50" stiffness="0"/>
      </default>

      <!-- tracking site -->
      <default class="tracking_site">
        <site type="sphere" size="0.027" rgba="0 0 1 1" group="3"/>
      </default>
    </default>
  </default>

  <worldbody>
    <geom name="floor" type="plane" conaffinity="1" size="50 50 .05" material="blue_grid"/>
    <body name="torso" pos="0 0 1.282" childclass="body">
      <light name="top" pos="0 0 2" mode="trackcom"/>
      <camera name="back" pos="-3 -0.2 1" xyaxes="0 -1 0 1 0 2" mode="trackcom"/>
      <camera name="side" pos="-0.75 -3 1" xyaxes="1 0 0 0 1 2" mode="trackcom"/>
      <freejoint name="root"/>
      <geom name="torso" fromto="0 -.07 0 0 .07 0" size=".07"/>
      <geom name="waist_upper" fromto="-.01 -.06 -.12 -.01 .06 -.12" size=".06"/>
      <body name="head" pos="0 0 .19">
        <geom name="head" type="sphere" size=".09"/>
        <site name="tracking[head]" class="tracking_site" pos="0.09 0 0" />
        <camera name="egocentric" pos=".09 0 0" xyaxes="0 -1 0 .1 0 1" fovy="80"/>
      </body>
      <body name="waist_lower" pos="-.01 0 -.26">
        <geom name="waist_lower" fromto="0 -.06 0 0 .06 0" size=".06"/>
        <joint name="abdomen_z" pos="0 0 .065" axis="0 0 1" range="-45 45" class="joint_big_stiff"/>
        <joint name="abdomen_y" pos="0 0 .065" axis="0 1 0" range="-75 30" class="joint_big"/>
        <body name="pelvis" pos="0 0 -.165">
          <site name="tracking[pelvis]" class="tracking_site" pos="0 0 0.075" size=".05" />
          <joint name="abdomen_x" pos="0 0 .1" axis="1 0 0" range="-35 35" class="joint_big"/>
          <geom name="butt" fromto="-.02 -.07 0 -.02 .07 0" size=".09"/>
          <body name="thigh_right" pos="0 -.1 -.04">
            <site name="tracking[rhip]" class="tracking_site" pos="0 0.025 0.025" />
            <joint name="hip_x_right" axis="1 0 0" class="hip_x"/>
            <joint name="hip_z_right" axis="0 0 1" class="hip_z"/>
            <joint name="hip_y_right" class="hip_y"/>
            <geom name="thigh_right" fromto="0 0 0 0 .01 -.34" class="thigh"/>
            <body name="shin_right" pos="0 .01 -.4">
              <joint name="knee_right" class="knee"/>
              <site name="tracking[rknee]" class="tracking_site" pos="0 0 0.05" />
              <geom name="shin_right" class="shin"/>
              <body name="foot_right" pos="0 0 -.39">
                <joint name="ankle_y_right" class="ankle_y"/>
                <joint name="ankle_x_right" class="ankle_x" axis="1 0 .5"/>
                <geom name="foot1_right" class="foot1"/>
                <geom name="foot2_right" class="foot2"/>
                <site name="foot_right" pos=".05 .03 0" type="sphere" size=".027"/>
                <site name="sp2" pos="-.07 0 0" type="sphere" size=".027"/>
                <site name="sp3" pos=".14 0 0" type="sphere" size=".027"/>
                <body name="heel_right" pos="-0.05 0 0.04">
                  <site name="tracking[rheel]" class="tracking_site" />
                </body>
                <body name="toe_right" pos="0.07 0 -0.01">
                  <site name="tracking[rtoe]" class="tracking_site" />
                </body>
              </body>
            </body>
          </body>
          <body name="thigh_left" pos="0 .1 -.04">
            <site name="tracking[lhip]" class="tracking_site" pos="0 -0.025 0.025" />
            <joint name="hip_x_left" axis="-1 0 0" class="hip_x"/>
            <joint name="hip_z_left" axis="0 0 -1" class="hip_z"/>
            <joint name="hip_y_left" class="hip_y"/>
            <geom name="thigh_left" fromto="0 0 0 0 -.01 -.34" class="thigh"/>
            <body name="shin_left" pos="0 -.01 -.4">
              <joint name="knee_left" class="knee"/>
              <site name="tracking[lknee]" class="tracking_site" pos="0 0 0.05" />
              <geom name="shin_left" fromto="0 0 0 0 0 -.3" class="shin"/>
              <body name="foot_left" pos="0 0 -.39">
                <joint name="ankle_y_left" class="ankle_y"/>
                <joint name="ankle_x_left" class="ankle_x" axis="-1 0 -.5"/>
                <geom name="foot1_left" class="foot1"/>
                <geom name="foot2_left" class="foot2"/>
                <site name="foot_left" pos=".05 -.03 0" type="sphere" size=".027"/>
                <site name="sp0" pos="-.07 0 0" type="sphere" size=".027"/>
                <site name="sp1" pos=".14 0 0" type="sphere" size=".027"/>
                <body name="heel_left" pos="-0.05 0 0.04">
                  <site name="tracking[lheel]" class="tracking_site" />
                </body>
                <body name="toe_left" pos="0.07 0 -0.01">
                  <site name="tracking[ltoe]" class="tracking_site" />
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
      <body name="upper_arm_right" pos="0 -.17 .06">
        <site name="tracking[rshoulder]" class="tracking_site" />
        <joint name="shoulder1_right" axis="2 1 1"  class="shoulder"/>
        <joint name="shoulder2_right" axis="0 -1 1" class="shoulder"/>
        <geom name="upper_arm_right" fromto="0 0 0 .16 -.16 -.16" class="arm_upper"/>
        <body name="lower_arm_right" pos=".18 -.18 -.18">
          <joint name="elbow_right" axis="0 -1 1" class="elbow"/>
          <site name="tracking[relbow]" class="tracking_site" />
          <site name="tracking[rhand]" class="tracking_site" pos="0.13 0.13 0.13" />
          <geom name="lower_arm_right" fromto=".01 .01 .01 .17 .17 .17" class="arm_lower"/>
          <body name="hand_right" pos=".18 .18 .18">
            <geom name="hand_right" zaxis="1 1 1" class="hand"/>
          </body>
        </body>
      </body>
      <body name="upper_arm_left" pos="0 .17 .06">
        <site name="tracking[lshoulder]" class="tracking_site" />
        <joint name="shoulder1_left" axis="-2 1 -1" class="shoulder"/>
        <joint name="shoulder2_left" axis="0 -1 -1"  class="shoulder"/>
        <geom name="upper_arm_left" fromto="0 0 0 .16 .16 -.16" class="arm_upper"/>
        <body name="lower_arm_left" pos=".18 .18 -.18">
          <joint name="elbow_left" axis="0 -1 -1" class="elbow"/>
          <site name="tracking[lelbow]" class="tracking_site" />
          <site name="tracking[lhand]" class="tracking_site" pos="0.13 -0.13 0.13" />
          <geom name="lower_arm_left" fromto=".01 -.01 .01 .17 -.17 .17" class="arm_lower"/>
          <body name="hand_left" pos=".18 -.18 .18">
            <geom name="hand_left" zaxis="1 -1 1" class="hand"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <contact>
    <exclude body1="waist_lower" body2="thigh_right"/>
    <exclude body1="waist_lower" body2="thigh_left"/>
  </contact>

  <tendon>
    <fixed name="hamstring_right" limited="true" range="-0.3 2">
      <joint joint="hip_y_right" coef=".5"/>
      <joint joint="knee_right" coef="-.5"/>
    </fixed>
    <fixed name="hamstring_left" limited="true" range="-0.3 2">
      <joint joint="hip_y_left" coef=".5"/>
      <joint joint="knee_left" coef="-.5"/>
    </fixed>
  </tendon>

  <actuator>
    <general name="abdomen_y" ctrllimited="true" ctrlrange="-1 1" forcerange="-300 300" joint="abdomen_y" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="274.8894" biasprm="-117.8097 -300 0"/>
    <general name="abdomen_z" ctrllimited="true" ctrlrange="-1 1" forcerange="-180 180" joint="abdomen_z" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="141.37164" biasprm="0 -180 0"/>
    <general name="abdomen_x" ctrllimited="true" ctrlrange="-1 1" forcerange="-200 200" joint="abdomen_x" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="122.173" biasprm="0 -200 0"/>
    <general name="hip_x_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-200 200" joint="hip_x_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="69.8132" biasprm="-34.90660 -200 0"/>
    <general name="hip_z_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-200 200" joint="hip_z_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="165.8063" biasprm="-43.63330 -200 0"/>
    <general name="hip_y_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-300 300" joint="hip_y_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="445.05900" biasprm="-340.3392 -300 0"/>
    <general name="knee_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-160 160" joint="knee_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="226.19472" biasprm="-220.6096 -160 0"/>
    <general name="ankle_x_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-50 50" joint="ankle_x_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="43.63325" biasprm="0 -50 0"/>
    <general name="ankle_y_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="ankle_y_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="104.71980" biasprm="0 -120 0"/>
    <general name="hip_x_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-200 200" joint="hip_x_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="69.8132" biasprm="-34.90660 -200 0"/>
    <general name="hip_z_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-200 200" joint="hip_z_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="165.8063" biasprm="-43.63330 -200 0"/>
    <general name="hip_y_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-300 300" joint="hip_y_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="445.05900" biasprm="-340.3392 -300 0"/>
    <general name="knee_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-160 160" joint="knee_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="226.19472" biasprm="-220.6096 -160 0"/>
    <general name="ankle_x_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-50 50" joint="ankle_x_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="43.63325" biasprm="0 -50 0"/>
    <general name="ankle_y_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="ankle_y_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="104.71980" biasprm="0 -120 0"/>
    <general name="shoulder1_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="shoulder1_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="188.4955861434" biasprm="-62.8318261434 -120 0"/>
    <general name="shoulder2_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="shoulder2_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="188.4955861434" biasprm="-62.8318261434 -120 0"/>
    <general name="elbow_right" ctrllimited="true" ctrlrange="-1 1" forcerange="-90 90" joint="elbow_right" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="117.80973" biasprm="-39.269879999999993 -90 0"/>
    <general name="shoulder1_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="shoulder1_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="188.4955861434" biasprm="-62.8318261434 -120 0"/>
    <general name="shoulder2_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-120 120" joint="shoulder2_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="188.4955861434" biasprm="-62.8318261434 -120 0"/>
    <general name="elbow_left" ctrllimited="true" ctrlrange="-1 1" forcerange="-90 90" joint="elbow_left" dyntype="filter" biastype="affine" dynprm="0.03" gainprm="117.80973" biasprm="-39.269878 -90 0"/>
  </actuator>

</mujoco>
