<mujoco model="HumanoidCMU Tracking">
  <include file="../../common.xml"/>
  <include file="../humanoid_CMU_V2020.xml" />
  <size memory="1M"/>

  <custom>
    <numeric name="task_transition" data="1" />
    <numeric name="agent_planner" data="0" />
    <numeric name="agent_horizon" data="0.6" />
    <numeric name="agent_timestep" data="0.015" />
    <numeric name="sampling_spline_points" data="5" />
    <numeric name="sampling_exploration" data="0.15" />
    <numeric name="gradient_spline_points" data="5" />
  </custom>

  <default>
    <default class="mocap_site">
      <site type="sphere" size="0.027" rgba="0 0 1 1" group="2" material="target"/>
    </default>
  </default>

  <worldbody>
    <geom name="floor" pos="0 0 -0.2" size="0 0 0.05" type="plane" material="blue_grid"/>

    <body name="mocap[pelvis]" mocap="true">
      <site name="mocap[pelvis]" class="mocap_site" size="0.05" />
    </body>
    <body name="mocap[head]" mocap="true">
      <site name="mocap[head]" class="mocap_site" />
    </body>
    <body name="mocap[ltoe]" mocap="true">
      <site name="mocap[ltoe]" class="mocap_site" />
    </body>
    <body name="mocap[rtoe]" mocap="true">
      <site name="mocap[rtoe]" class="mocap_site" />
    </body>
    <body name="mocap[lheel]" mocap="true">
      <site name="mocap[lheel]" class="mocap_site" />
    </body>
    <body name="mocap[rheel]" mocap="true">
      <site name="mocap[rheel]" class="mocap_site" />
    </body>
    <body name="mocap[lknee]" mocap="true">
      <site name="mocap[lknee]" class="mocap_site" />
    </body>
    <body name="mocap[rknee]" mocap="true">
      <site name="mocap[rknee]" class="mocap_site" />
    </body>
    <body name="mocap[lhand]" mocap="true">
      <site name="mocap[lhand]" class="mocap_site" />
    </body>
    <body name="mocap[rhand]" mocap="true">
      <site name="mocap[rhand]" class="mocap_site" />
    </body>
    <body name="mocap[lelbow]" mocap="true">
      <site name="mocap[lelbow]" class="mocap_site" />
    </body>
    <body name="mocap[relbow]" mocap="true">
      <site name="mocap[relbow]" class="mocap_site" />
    </body>
    <body name="mocap[lshoulder]" mocap="true">
      <site name="mocap[lshoulder]" class="mocap_site" />
    </body>
    <body name="mocap[rshoulder]" mocap="true">
      <site name="mocap[rshoulder]" class="mocap_site" />
    </body>
    <body name="mocap[lhip]" mocap="true">
      <site name="mocap[lhip]" class="mocap_site" />
    </body>
    <body name="mocap[rhip]" mocap="true">
      <site name="mocap[rhip]" class="mocap_site" />
    </body>
  </worldbody>

  <sensor>
    <user name="control" dim="56" user="0 0.1 0 1.0" />
    <user name="joint vel." dim="62" user="0 0.01 0.0 1.0" />

    <user name="root pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="head pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="ltoe pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rtoe pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lheel pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rheel pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lknee pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="rknee pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="lhand pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rhand pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lelbow pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="relbow pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lshoulder pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="rshoulder pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="lhip pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rhip pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />

    <user name="root vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="head vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="ltoe vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rtoe vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lheel vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rheel vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lknee vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rknee vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lhand vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rhand vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lelbow vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="relbow vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lshoulder vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rshoulder vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lhip vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rhip vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />

    <framepos name="trace" objtype="body" objname="thorax"/>

    <framepos name="tracking_pos[pelvis]" objtype="site" objname="root"/>
    <framepos name="tracking_pos[head]" objtype="site" objname="tracking[head]"/>
    <framepos name="tracking_pos[ltoe]" objtype="site" objname="tracking[ltoes0]"/>
    <framepos name="tracking_pos[rtoe]" objtype="site" objname="tracking[rtoes0]"/>
    <framepos name="tracking_pos[lheel]" objtype="site" objname="tracking[lfoot]"/>
    <framepos name="tracking_pos[rheel]" objtype="site" objname="tracking[rfoot]"/>
    <framepos name="tracking_pos[lknee]" objtype="site" objname="tracking[ltibia]"/>
    <framepos name="tracking_pos[rknee]" objtype="site" objname="tracking[rtibia]"/>
    <framepos name="tracking_pos[lhand]" objtype="site" objname="tracking[lwrist]"/>
    <framepos name="tracking_pos[rhand]" objtype="site" objname="tracking[rwrist]"/>
    <framepos name="tracking_pos[lelbow]" objtype="site" objname="tracking[lradius]"/>
    <framepos name="tracking_pos[relbow]" objtype="site" objname="tracking[rradius]"/>
    <framepos name="tracking_pos[lshoulder]" objtype="site" objname="tracking[lhumerus]"/>
    <framepos name="tracking_pos[rshoulder]" objtype="site" objname="tracking[rhumerus]"/>
    <framepos name="tracking_pos[lhip]" objtype="site" objname="tracking[lfemur]"/>
    <framepos name="tracking_pos[rhip]" objtype="site" objname="tracking[rfemur]"/>

    <framelinvel name="tracking_linvel[pelvis]" objtype="site" objname="root"/>
    <framelinvel name="tracking_linvel[head]" objtype="site" objname="tracking[head]"/>
    <framelinvel name="tracking_linvel[ltoe]" objtype="site" objname="tracking[ltoes0]"/>
    <framelinvel name="tracking_linvel[rtoe]" objtype="site" objname="tracking[rtoes0]"/>
    <framelinvel name="tracking_linvel[lheel]" objtype="site" objname="tracking[lfoot]"/>
    <framelinvel name="tracking_linvel[rheel]" objtype="site" objname="tracking[rfoot]"/>
    <framelinvel name="tracking_linvel[lknee]" objtype="site" objname="tracking[ltibia]"/>
    <framelinvel name="tracking_linvel[rknee]" objtype="site" objname="tracking[rtibia]"/>
    <framelinvel name="tracking_linvel[lhand]" objtype="site" objname="tracking[lwrist]"/>
    <framelinvel name="tracking_linvel[rhand]" objtype="site" objname="tracking[rwrist]"/>
    <framelinvel name="tracking_linvel[lelbow]" objtype="site" objname="tracking[lradius]"/>
    <framelinvel name="tracking_linvel[relbow]" objtype="site" objname="tracking[rradius]"/>
    <framelinvel name="tracking_linvel[lshoulder]" objtype="site" objname="tracking[lhumerus]"/>
    <framelinvel name="tracking_linvel[rshoulder]" objtype="site" objname="tracking[rhumerus]"/>
    <framelinvel name="tracking_linvel[lhip]" objtype="site" objname="tracking[lfemur]"/>
    <framelinvel name="tracking_linvel[rhip]" objtype="site" objname="tracking[rfemur]"/>
  </sensor>

