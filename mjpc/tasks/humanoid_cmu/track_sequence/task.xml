<mujoco model="HumanoidCMU Tracking">
  <include file="../../common.xml"/>
  <include file="../humanoid_CMU_V2020.xml" />
  <size memory="1M"/>

  <custom>
    <numeric name="task_transition" data="1" />
    <numeric name="agent_planner" data="0" />
    <numeric name="agent_horizon" data="0.6" />
    <numeric name="agent_timestep" data="0.015" />
    <numeric name="sampling_spline_points" data="5" />
    <numeric name="sampling_exploration" data="0.15" />
    <numeric name="gradient_spline_points" data="5" />
  </custom>

  <worldbody>
    <geom name="floor" pos="0 0 -0.2" size="0 0 0.05" type="plane" material="blue_grid"/>

    <body name="mocap-pelvis" mocap="true">
      <site name="mocap-pelvis" type="sphere" size="0.05" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-head" mocap="true">
      <site name="mocap-head" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-ltoe" mocap="true">
      <site name="mocap-ltoe" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rtoe" mocap="true">
      <site name="mocap-rtoe" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lheel" mocap="true">
      <site name="mocap-lheel" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rheel" mocap="true">
      <site name="mocap-rheel" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lknee" mocap="true">
      <site name="mocap-lknee" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rknee" mocap="true">
      <site name="mocap-rknee" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lhand" mocap="true">
      <site name="mocap-lhand" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rhand" mocap="true">
      <site name="mocap-rhand" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lelbow" mocap="true">
      <site name="mocap-lelbow" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-relbow" mocap="true">
      <site name="mocap-relbow" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lshoulder" mocap="true">
      <site name="mocap-lshoulder" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rshoulder" mocap="true">
      <site name="mocap-rshoulder" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-lhip" mocap="true">
      <site name="mocap-lhip" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
    <body name="mocap-rhip" mocap="true">
      <site name="mocap-rhip" type="sphere" size="0.03" group="0" rgba="0 0 1 1" material="target" />
    </body>
  </worldbody>

  <sensor>
    <user name="control" dim="56" user="0 0.1 0 1.0" />
    <user name="joint vel." dim="62" user="0 0.01 0.0 1.0" />

    <user name="root pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="head pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="ltoe pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rtoe pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lheel pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rheel pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lknee pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="rknee pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="lhand pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rhand pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lelbow pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="relbow pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="lshoulder pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="rshoulder pos error" dim="3" user="6 10.0 0.0 100.0 0.1" />
    <user name="lhip pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />
    <user name="rhip pos error" dim="3" user="6 1.0 0.0 100.0 0.1" />

    <user name="root vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="head vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="ltoe vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rtoe vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lheel vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rheel vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lknee vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rknee vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lhand vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rhand vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lelbow vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="relbow vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lshoulder vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rshoulder vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="lhip vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />
    <user name="rhip vel error" dim="3" user="6 0.01 0.0 100.0 0.1" />

    <framepos name="trace" objtype="body" objname="thorax"/>

    <framepos name="tracking_pose[pelvis]" objtype="site" objname="root"/>
    <framepos name="tracking_pose[head]" objtype="site" objname="tracking-head"/>
    <framepos name="tracking_pose[ltoe]" objtype="site" objname="tracking-ltoes0"/>
    <framepos name="tracking_pose[rtoe]" objtype="site" objname="tracking-rtoes0"/>
    <framepos name="tracking_pose[lheel]" objtype="site" objname="tracking-lfoot"/>
    <framepos name="tracking_pose[rheel]" objtype="site" objname="tracking-rfoot"/>
    <framepos name="tracking_pose[lknee]" objtype="site" objname="tracking-ltibia"/>
    <framepos name="tracking_pose[rknee]" objtype="site" objname="tracking-rtibia"/>
    <framepos name="tracking_pose[lhand]" objtype="site" objname="tracking-lwrist"/>
    <framepos name="tracking_pose[rhand]" objtype="site" objname="tracking-rwrist"/>
    <framepos name="tracking_pose[lelbow]" objtype="site" objname="tracking-lradius"/>
    <framepos name="tracking_pose[relbow]" objtype="site" objname="tracking-rradius"/>
    <framepos name="tracking_pose[lshoulder]" objtype="site" objname="tracking-lhumerus"/>
    <framepos name="tracking_pose[rshoulder]" objtype="site" objname="tracking-rhumerus"/>
    <framepos name="tracking_pose[lhip]" objtype="site" objname="tracking-lfemur"/>
    <framepos name="tracking_pose[rhip]" objtype="site" objname="tracking-rfemur"/>

    <framelinvel name="tracking_vel[pelvis]" objtype="site" objname="root"/>
    <framelinvel name="tracking_vel[head]" objtype="site" objname="tracking-head"/>
    <framelinvel name="tracking_vel[ltoe]" objtype="site" objname="tracking-ltoes0"/>
    <framelinvel name="tracking_vel[rtoe]" objtype="site" objname="tracking-rtoes0"/>
    <framelinvel name="tracking_vel[lheel]" objtype="site" objname="tracking-lfoot"/>
    <framelinvel name="tracking_vel[rheel]" objtype="site" objname="tracking-rfoot"/>
    <framelinvel name="tracking_vel[lknee]" objtype="site" objname="tracking-ltibia"/>
    <framelinvel name="tracking_vel[rknee]" objtype="site" objname="tracking-rtibia"/>
    <framelinvel name="tracking_vel[lhand]" objtype="site" objname="tracking-lwrist"/>
    <framelinvel name="tracking_vel[rhand]" objtype="site" objname="tracking-rwrist"/>
    <framelinvel name="tracking_vel[lelbow]" objtype="site" objname="tracking-lradius"/>
    <framelinvel name="tracking_vel[relbow]" objtype="site" objname="tracking-rradius"/>
    <framelinvel name="tracking_vel[lshoulder]" objtype="site" objname="tracking-lhumerus"/>
    <framelinvel name="tracking_vel[rshoulder]" objtype="site" objname="tracking-rhumerus"/>
    <framelinvel name="tracking_vel[lhip]" objtype="site" objname="tracking-lfemur"/>
    <framelinvel name="tracking_vel[rhip]" objtype="site" objname="tracking-rfemur"/>
  </sensor>

